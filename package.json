{
  "private": true,
  "name": "futurememories",
  "version": "1.0.0",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build --prerender --template src/template.html",
    "postbuild": "node postbuild.js",
    "serve": "preact build --prerender --template src/template.html && preact serve",
    "dev": "preact watch",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^4.6.0",
    "eslint": "^5.16.0",
    "eslint-config-synacor": "^3.0.4",
    "if-env": "^1.0.4",
    "node-sass": "^4.12.0",
    "preact-cli": "^2.2.1",
    "preact-cli-workbox-plugin": "^1.1.1",
    "sass-loader": "^7.3.1",
    "sine-waves": "^0.3.0"
  },
  "dependencies": {
    "in-view": "git+https://github.com/losthumans/in-view.git",
    "preact": "^8.5.2",
    "preact-compat": "^3.19.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "build"
    ]
  }
}
