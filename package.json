{
  "name": "FutureMemoriesWeb",
  "version": "1.0.0",
  "scripts": {
    "start": "parcel src/index.pug",
    "build": "rm -rf dist && parcel build src/index.pug --public-url / && inline-source --root ./dist dist/index.html dist/index.html && cp -r static/* dist/."
  },
  "dependencies": {
    "browsernizr": "^2.2.0",
    "debounce": "^1.1.0",
    "inline-source-cli": "^1.2.0",
    "node-sass": "^4.7.2",
    "parcel-bundler": "^1.6.2",
    "parcel-plugin-pug": "^0.2.5",
    "smoothscroll": "^0.4.0"
  },
  "browserslist": [
    "last 2 versions"
  ]
}
